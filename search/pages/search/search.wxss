.my-container{
  --button-default-background-color:#f5f5f5;
  --button-default-border-color: #f5f5f5 ;
  --van-button-small-padding:0;
}
.my-search-container{
  display: flex;
  margin: 20rpx 20rpx;
}
.search-input{
  width: 630rpx;
  font-size: 30rpx;
  padding: 8rpx 0;
  text-align: center;
  border-radius: 40rpx;
  background-color: #fffffb;
}

.my-before-search{
height: 800rpx;
}
.my-before{
  display: flex;
  justify-content: space-between;
  margin-left: 20rpx;
}
.before-search-box{

  margin-top: 50rpx;
  padding: 20rpx 0;
  display: flex;
  flex-wrap: wrap;
  justify-content: right;
}
.history-box{
  height: 50rpx;
  margin-left:20rpx;
  margin-top: 20rpx;
  background-color: rgba(0, 0, 0, .4);
  border-radius: 10rpx;
  padding:0 20rpx;
  color: #fffffb;

}
.associate-box{
  margin-left: 20rpx;
  line-height:80rpx ;
}
.search-bar-box{
  background-color: #fffffb;
}
.songs-bar{
  width: 80rpx;
  margin-left: 40rpx;
  color: aquamarine;
  font-size: 40rpx;
  border-bottom:8rpx solid aquamarine;
  padding-bottom: 15rpx;
}

.songs-name-box{
  height: 120rpx;
  margin-top: 50rpx;
  margin-left: 40rpx;
}
